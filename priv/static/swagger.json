          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "items": [
                    {
                      "description": "fields available for `ACCOUNT_OWNER` permission",
                      "example": {
                        "amount": 10000,
                        "operation": "DEBIT",
                        "refunded?": true,
                        "refunded_at": "2023-08-21T22:13:03Z"
                      },
                      "properties": {
                        "amount": {
                          "description": "amount",
                          "example": 10000,
                          "type": "integer"
                        },
                        "inserted_at": {
                          "type": "utc_datetime"
                        },
                        "operation": {
                          "description": "operation of this transaction",
                          "enum": [
                            "DEBIT",
                            "CREDIT"
                          ],
                          "example": "DEBIT",
                          "type": "Elixir.Ecto.Enum"
                        },
                        "payed_to": {
                          "type": "binary_id"
                        },
                        "received_from": {
                          "type": "binary_id"
                        },
                        "refunded?": {
                          "description": "flag to mark if was refunded",
                          "example": true,
                          "type": "boolean"
                        },
                        "refunded_at": {
                          "description": "DateTime when the transaction was refunded",
                          "example": "2023-08-21T22:13:03Z",
                          "type": "utc_datetime"
                        },
                        "updated_at": {
                          "type": "utc_datetime"
                        }
                      },
                      "required": [],
                      "title": "Consolidated Transaction Entity",
                      "type": "object"
                    }
                  ],
                  "type": "array"
                }
              }
            },
            "description": "ok"
          }
        },
        "summary": "Get consolidations from account",
        "tags": [
          "accounts"
        ]
